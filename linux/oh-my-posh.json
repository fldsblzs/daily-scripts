{
    "final_space": true,
    "console_title": false,
    "console_title_style": "",
    "console_title_template": "",
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "horizontal_offset": 0,
        "vertical_offset": 0,
        "segments": [
          {
            "type": "shell",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#ffffff",
            "background": "#0077c2",
            "properties": {
              "prefix": " \uFCB5 "
            }
          },
          {
            "type": "session",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "invert_powerline": false,
            "foreground": "#ffffff",
            "background": "#0077c2",
            "properties": null
          },
          {
            "type": "time",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#ffffff",
            "background": "#0077c2",
            "properties": {
              "time_format": "15:04:05",
              "prefix": "[",
              "postfix": "]"
            }
          },
          {
            "type": "kubectl",
            "style": "diamond",
            "foreground": "#000000",
            "background": "#6CA35E",
            "leading_diamond": "",
            "trailing_diamond": "",
            "properties": {
              "prefix": "  "
            }
          },
          {
            "type": "path",
            "style": "powerline",
            "powerline_symbol": "",
            "invert_powerline": false,
            "foreground": "#ffffff",
            "background": "#3F7348",
            "properties": {
              "prefix": "  ",
              "style": "full"
            }
          },
          {
            "type": "git",
            "style": "powerline",
            "powerline_symbol": "",
            "invert_powerline": false,
            "foreground": "#193549",
            "background": "#fffb38",
            "properties": {
              "display_stash_count": true,
              "display_upstream_icon": true
            }
          },
          {
            "type": "root",
            "style": "powerline",
            "powerline_symbol": "",
            "invert_powerline": false,
            "foreground": "#193549",
            "background": "#ffff66",
            "leading_diamond": "",
            "trailing_diamond": "",
            "properties": null
          },
          {
            "type": "exit",
            "style": "diamond",
            "powerline_symbol": "",
            "invert_powerline": false,
            "foreground": "#ffffff",
            "background": "#2e9599",
            "leading_diamond": "",
            "trailing_diamond": "",
            "properties": {
              "always_enabled": true,
              "color_background": true,
              "display_exit_code": false,
              "error_color": "#f1184c",
              "prefix": "\u003ctransparent\u003e\u003c/\u003e "
            }
          }
        ]
      }
    ]
  }
  